{
  "name": "@antv/thumbnails",
  "version": "1.3.0",
  "description": "Thumbnails of Chart Types.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "browser": "dist/index.js",
  "module": "es/src/index.js",
  "files": [
    "src",
    "es",
    "dist",
    "lib"
  ],
  "scripts": {
    "start": "webpack-dev-server --port 8712",
    "module": "rm -rf es && tsc -t ES5 -m ESNext --outDir es",
    "build": "rm -rf lib && tsc --pretty",
    "extractsvg": "node lib/scripts/extract-svg.js",
    "readmepreview": "node lib/scripts/readme-preview.js",
    "test:gen": "npm run build && node lib/scripts/generate.js && node lib/scripts/headless.js",
    "update": "npm run build && npm run extractsvg && npm run readmepreview && npm run build",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublish": "npm run module && npm run update && npm run pack",
    "pack": "webpack --config=webpack.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prepublish && npm test && npm run lint && webpack --mode=production",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:antvis/thumbnails.git"
  },
  "keywords": [
    "AntV",
    "Chart",
    "Visualization",
    "Thumbnails",
    "Design"
  ],
  "author": "AFX",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@antv/knowledge": "^0.1.3-alpha.16",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/runtime": "^7.6.3",
    "@babel/types": "^7.7.4",
    "@types/fs-extra": "^9.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^24.0.24",
    "@types/puppeteer": "^5.4.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/svgo": "^1.3.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^5.0.1",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "jest": "^24.9.0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "prettier": "^1.19.1",
    "puppeteer": "^10.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.0.0",
    "svgo": "^1.3.2",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.11.1"
  }
}
