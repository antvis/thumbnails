{
  "name": "@antv/thumbnails",
  "version": "2.0.0-alpha.1",
  "description": "Thumbnails of Chart Types.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "browser": "dist/index.js",
  "module": "esm/src/index.js",
  "files": [
    "src",
    "esm",
    "dist",
    "lib"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\" \"demo/**/*.{ts,tsx}\"",
    "format-check": " prettier ./src/**/*.ts ./__tests__/**/*.ts ./demo/**/*.{ts,tsx} --check",
    "lint": "eslint ./src/**/*.ts ./__tests__/**/*.ts ./demo/**/*.{ts,tsx} && npm run format-check",
    "fix": "eslint ./src/**/*.ts ./__tests__/**/*.ts ./demo/**/*.{ts,tsx} --fix && npm run format",
    "lint-staged": "lint-staged",
    "size": "limit-size",
    "clean": "rimraf lib esm dist",
    "build:umd": "rimraf ./dist && rollup -c && npm run size",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*",
    "test": "jest",
    "ci": "run-s lint schema test build",
    "prepare:demo": "cd demo && npm install --package-lock=false",
    "start:demo": "npm run prepare:demo && cd demo && npm start",
    "prepare": "husky install",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "extractsvg": "node lib/scripts/extract-svg.js",
    "readmepreview": "node lib/scripts/readme-preview.js",
    "test:gen": "npm run build && node lib/scripts/generate.js && node lib/scripts/headless.js",
    "update": "npm run build && npm run extractsvg && npm run readmepreview && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:antvis/thumbnails.git"
  },
  "keywords": [
    "AntV",
    "Chart",
    "Visualization",
    "Thumbnails",
    "Design"
  ],
  "author": {
    "name": "AntV",
    "url": "https://antv.vision/"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@antv/knowledge": "^1.1.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/fs-extra": "^9.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^24.0.24",
    "@types/puppeteer": "^5.4.4",
    "@types/svgo": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "fs-extra": "^9.0.1",
    "husky": "^7.0.1",
    "inquirer": "^7.3.3",
    "jest": "^24.9.0",
    "limit-size": "^0.1.4",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "puppeteer": "^10.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.0",
    "rollup-plugin-terser": "^7.0.2",
    "svgo": "^1.3.2",
    "ts-jest": "^24.2.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@antv/knowledge": "^1.1.4",
    "@babel/runtime": "^7.14.8",
    "react": "^17.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "limit-size": [
    {
      "path": "dist/index.min.js",
      "limit": "8 Kb",
      "gzip": true
    },
    {
      "path": "dist/index.min.js",
      "limit": "24 Kb"
    }
  ]
}
