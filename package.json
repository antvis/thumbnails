{
  "name": "@antv/thumbnails",
  "version": "0.3.0",
  "description": "Thumbnails of Chart Types.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "src",
    "es",
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "rm -rf lib && tsc --pretty",
    "module": "rm -rf es && tsc -t ES5 -m ESNext --outDir es",
    "prepublish": "npm run module && npm run build && npm run pack",
    "pack": "webpack --config=webpack.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.alipay-inc.com:datavis/thumbnails.git"
  },
  "keywords": [
    "AntV",
    "Chart",
    "Visualization",
    "Thumbnails",
    "Design"
  ],
  "author": "AFX",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@antv/knowledge": "^0.1.3-alpha.16",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/runtime": "^7.6.3",
    "@babel/types": "^7.7.4",
    "@types/jest": "^24.0.24",
    "babel-loader": "^8.0.6",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
